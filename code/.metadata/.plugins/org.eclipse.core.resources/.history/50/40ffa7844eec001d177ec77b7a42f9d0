#ifndef MOTORS_H
#define MOTORS_H

typedef enum{
	FORWARD,
	BACKWARD
}motor_direction;

void motor_L_set_speed(uint8_t speed);
void motor_R_set_speed(uint8_t speed);
void motor_L_set_direction(motor_direction direction);
void motor_R_set_direction(motor_direction direction);

#endif
